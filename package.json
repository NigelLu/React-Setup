{
  "name": "react-setup",
  "version": "1.0.0",
  "description": "basic setup for reactjs development, with webpack configured",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "start": "webpack-dev-server --mode development"
  },
  "lint-staged": {
    "tests/**/*.{js,jsx,ts,tsx,json,css,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,less,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NigelLu/React-Setup.git"
  },
  "keywords": [
    "d3.js"
  ],
  "author": "NigelLu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NigelLu/React-Setup/issues"
  },
  "homepage": "https://github.com/NigelLu/React-Setup#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "d3": "^7.8.2"
  }
}
